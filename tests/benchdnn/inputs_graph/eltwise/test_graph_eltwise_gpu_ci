--alg=abs,exp,gelu_erf,log,round,sqrt
64x16x28x28

--alpha=0 --beta=2
--alg=clip_v2
64x16x28x28

--alpha= --beta=0
--alg=elu
64x16x28x28

#all supported algs bwd
--reset
--inplace=false
--dir=BWD_D
--dt=f32
--tag=axb
--alpha=0 --beta=0
