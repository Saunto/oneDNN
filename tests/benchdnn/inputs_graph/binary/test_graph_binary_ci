# main op
--reset
--alg=ADD,MUL,MAX,MIN --stag=abx:abx 64x128x28x28:64x128x28x28

# post binary
--reset
--alg=MUL --ddt=f32 --sdt=f32:f32 --attr-post-ops=add:f32:common --stag=abx:abx 64x128x28x28:64x128x28x28

# post sum
--reset
--alg=MAX --attr-post-ops=sum --stag=abx:abx 64x128x28x28:64x128x28x28

# post eltwise
--reset
--alg=MIN --ddt=f32 --sdt=f32:f32 --attr-post-ops=logistic --stag=abx:abx 64x128x28x28:64x128x28x28