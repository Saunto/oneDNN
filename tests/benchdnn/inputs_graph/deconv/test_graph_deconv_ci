# no groups
--reset
--dir=FWD_D
--stag=abc
--wtag=abcd
--dtag=abc
--batch=shapes_1d

--reset
--dir=FWD_D
--stag=abcd
--wtag=abcde
--dtag=abcd
--batch=shapes_2d

--reset
--dir=FWD_D
--stag=abcde
--wtag=abcdef
--dtag=abcde
--batch=shapes_3d

# with groups
--reset
--dir=FWD_D
--stag=abc
--wtag=acbd
--dtag=abc
--batch=shapes_1d_groups

--reset
--dir=FWD_D
--stag=abcd
--wtag=acbde
--dtag=abcd
--batch=shapes_2d_groups

--reset
--dir=FWD_D
--stag=abcde
--wtag=acbdef
--dtag=abcde
--batch=shapes_3d_groups
